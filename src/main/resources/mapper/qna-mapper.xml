<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="qna">
 	<insert id="insertQna">
		insert into qna values(qna_SEQ.nextVal,#{qna_writer},default,#{store_pk},#{member_pk},#{qna_title},#{qna_content},sysdate,default,default,null)
	</insert>
</mapper>
